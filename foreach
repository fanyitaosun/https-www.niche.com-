// 遍历分页
foreach ($array as $key => $value) {
    $json = file_get_contents($value);
    print_r('<pre>');
    $school_array = json_decode($json,true)['entities'];
// 	print_r($school_array = json_decode($json,true)['entities']);
// 	die;
    // 	遍历学校
	foreach ($school_array as $s_key => $s_value) {
	    print_r('<br>');
	    print_r('https://www.niche.com/k12/' . $s_value['content']['entity']['url'] . '/' );
	    
	}
	
}
// file_get_contents

